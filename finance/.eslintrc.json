{
    "env": {
        "es2021": true,
        "node": true,
        "commonjs": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "import/extensions": ["error", "ignorePackages"],
        "import/no-unresolved": "off",
        "indent": ["error", 4],
        "new-cap": ["error", { "newIsCap": false }],
        "comma-dangle": ["error", "never"],
        "no-console": 0
    }
}
